{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/:path*"
    }
  ],
  "env": {
    "MONGODB_URI": "MONGODB_URI",
    "AUTH_SECRET": "Auth_secret",
    "GOOGLE_CLIENT_ID": "google-client-id",
    "GOOGLE_CLIENT_SECRET": "google-client-secret",
    "VERCEL_CLIENT_ID": "vercel-client-id",
    "VERCEL_CLIENT_SECRET": "vercel-client-secret"
  }
}
